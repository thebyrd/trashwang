{
  "/": {
    "get": "users.signup"
  },

  "/users": {
    "post": "users.create"
  },
  "/login": {
    "get": "users.login",
    "post": "users.loginPost"
  },
  "/logout": {
    "get": "users.logout"
  },
  "/parties": {
    "get": "parties.index",
    "post": "parties.create"
  },
  "/new-party": {
    "get": "parties.new"
  },
  "/parties/:partyId": {
    "get": "parties.show",
    "post": "parties.update"
  },
  "/_/upload": {
    "post": "parties.uploadImage"
  }
}
